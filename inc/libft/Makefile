NAME = libft.a

SRC = 		ft_strlen.c \
			ft_atoi.c \
 			ft_bzero.c \
  			ft_isalnum.c \
   			ft_isalpha.c \
    		ft_isascii.c \
	 		ft_isdigit.c \
			ft_isprint.c \
			ft_memchr.c \
			ft_memcmp.c \
			ft_memcpy.c \
			ft_memmove.c \
			ft_memset.c \
			ft_strchr.c \
			ft_tolower.c \
			ft_toupper.c \
			ft_strdup.c \
			ft_strrchr.c \
			ft_strncmp.c \
			ft_isprint.c \
			ft_strlcat.c \
			ft_strnstr.c \
			ft_strlcpy.c \
			ft_calloc.c \
			ft_putnbr_fd.c \
			ft_putchar_fd.c \
			ft_putendl_fd.c \
			ft_putstr_fd.c \
			ft_substr.c \
			ft_strjoin.c \
			ft_strtrim.c \
			ft_strmapi.c \
			ft_striteri.c \
			ft_split.c \
			ft_itoa.c \
			ft_printf.c \
			ft_putchar.c \
			ft_putnbr_hex.c \
			ft_putnbr.c \
			ft_putstr.c \
			ft_strlen.c \

SRCBONUS = 	ft_lstnew.c \
			ft_lstadd_front.c \
			ft_lstsize.c \
			ft_lstlast.c \
			ft_lstadd_back.c \
			ft_lstdelone.c \
			ft_lstclear.c \
			ft_lstiter.c \
			ft_lstdellast.c \

OBJECTS = $(SRC:.c=.o)
OBJECTSBONUS = $(SRCBONUS:.c=.o)

CC = gcc

CFLAGS = -Wall -Werror -Wextra

all: $(NAME)

$(NAME): $(OBJECTS)
		@$(AR) -rc $@ $?
		
bonus: $(OBJECTSBONUS)
		@$(AR) -rc $(NAME) $?

%.o: %.c
		@$(CC) -c $(FLAGS) $?

clean:
		@rm -f $(OBJECTS) $(OBJECTSBONUS)

fclean: clean
		@rm -f $(NAME)

re: fclean all

.PHONY: all clean fclean re test

